<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homebackground"
    >


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_200sdp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.116"
        app:srcCompat="@drawable/performancehoome" />

    <com.ebanx.swipebtn.SwipeButton
        android:id="@+id/swipe_btn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginLeft="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_80sdp"
        android:layout_marginRight="@dimen/_50sdp"
        app:button_background="@drawable/shape_button"
        app:button_bottom_padding="@dimen/_10sdp"
        app:button_image_disabled="@drawable/fanbtn"
        app:button_image_enabled="@drawable/fanbtn"
        app:button_left_padding="@dimen/_10sdp"
        app:button_right_padding="@dimen/_10sdp"
        app:button_top_padding="@dimen/_10sdp"
        app:inner_text="Clean"
        app:inner_text_background="@drawable/shape_rounded"
        app:inner_text_bottom_padding="@dimen/_16sdp"
        app:inner_text_color="@color/textcolor"
        app:inner_text_size="16sp"
        app:inner_text_top_padding="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:initial_state="disabled"
        app:button_trail_enabled="true"
        app:button_trail_drawable="@drawable/rounded_view"
        />

    <com.ebanx.swipebtn.SwipeButton
        android:id="@+id/swipe_btn1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginLeft="@dimen/_50sdp"
        android:layout_marginRight="@dimen/_50sdp"
        app:button_background="@drawable/shape_button"
        app:button_bottom_padding="@dimen/_10sdp"
        app:button_image_disabled="@drawable/phonepath"
        app:button_image_enabled="@drawable/phonepath"
        app:button_left_padding="@dimen/_10sdp"
        app:button_right_padding="@dimen/_10sdp"
        app:button_top_padding="@dimen/_10sdp"
        app:inner_text="Storage"
        app:inner_text_background="@drawable/shape_rounded"
        app:inner_text_bottom_padding="@dimen/_16sdp"
        app:inner_text_color="@color/textcolor"
        app:inner_text_size="16sp"
        app:inner_text_top_padding="@dimen/_16sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swipe_btn"
        app:layout_constraintVertical_bias="0.284"
        app:initial_state="disabled"
        app:button_trail_enabled="true"
        app:button_trail_drawable="@drawable/rounded_view"
        />

</androidx.constraintlayout.widget.ConstraintLayout>